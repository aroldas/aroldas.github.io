.topnav {
	text-align: center;
	width: 100%;
	overflow: show;
	background: rgb(10, 10, 10);
	position: fixed; 
    top: 0; 
	left: 0; 
}
body {
	background: rgb(10, 10, 10);
	height: 100%;
	font-family: 'Merriweather', serif;
	color: rgb(249, 246, 238);
	letter-spacing: 0.50vw;
}
.topnav a {
	text-decoration: none;
	text-align: center;
	display: flex;
	color: rgb(249, 246, 238);
	font-size: 1vw;
	padding: 0vw 8vw;
	padding-top: 2vw;
	padding-bottom: 2vw;
	float: left;
}
.topnav .icon {
	display: none;
}
.landing {
	text-align: center;
	font-size: 2vw;
	padding-top: 20vw;
	padding-bottom: 100vw;
	background-image: url("../images/landing.jpg");
	background-repeat: no-repeat;
	background-position: top center;
	background-size: cover;
	background-attachment: fixed;
	width: 100%;
	text-shadow: 0px 0px 25px rgba(128, 128, 128, 1),0px 0px 25px rgba(255, 255, 255, 1);
}
.active a {
	text-shadow: 0px 0px 25px rgba(128, 128, 128, 1),0px 0px 25px rgba(255, 255, 255, 1);
}
.topnav a:hover{
	text-shadow: 0px 0px 25px rgba(128, 128, 128, 1),0px 0px 25px rgba(255, 255, 255, 1);
}
.footer {
	min-width: 100%;
	width: 100%;
	background: rgb(10, 10, 10);
	text-align: center;
	left: 0;
	display: block;
    bottom: 0;
	color: white;
	position: fixed;
}
.footer a {
	padding: 1vw 8.1vw;
	color: rgb(249, 246, 238);
	text-decoration: none;
	display: inline-block;
	font-size: 1vw;
}
@media screen and (max-width: 654px) {
  .topnav a {
	padding: 1vw 8.1vw;
  }
}
@media screen and (max-width: 600px) {
  .topnav a:not(:first-child)  {display: none;}
  .topnav a.icon {
	float: right;
    display: block;
  }
@media screen and (max-width: 600px) {
  .topnav.responsive {position: fixed;}
  .topnav.responsive a.icon {
    position: absolute;
    right: 0;
    top: 0;
  }
  .topnav.responsive a {
    float: none;
    display: block;
    text-align: left;
  }
  .topnav a {
	font-size: 6vw;
	padding-top: 4vw;
	padding-bottom: 4vw;
  }
   .topnav {
	width: 100%;
  }
  .landing {
	margin-top: 10vw;
	margin-bottom: 10vw;	
	font-size: 5vw;
  }
  .footer a {
	font-size: 4vw;
  }
  }
}